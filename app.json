{
  "expo": {
    "name": "twitter-clone-mobile",
    "slug": "twitter-clone-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/twitterIcon.png",
    "primaryColor": "#1DA1F2",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/twitterSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1DA1F2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/twitter-adaptive-icon.png",
        "backgroundColor": "#1DA1F2"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
